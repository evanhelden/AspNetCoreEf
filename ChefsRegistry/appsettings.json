  "Serilog": {
    "Using": [ "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": "Warning",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Server=VANHEL;Database=Chef;Trusted_Connection=True;TrustServerCertificate=true;",
          "tableName": "LogEvents",
          "autoCreateSqlTable": false
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "MvcApp"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SqlConnection": "Server=VANHEL;Database=Chef;Trusted_Connection=True;TrustServerCertificate=true;"
  }
}

